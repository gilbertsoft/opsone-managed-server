{
	"name": "gilbertsoft/opsone-managed-server-project",
	"description": "Project to test the gilbertsoft/opsone-managed-server package.",
	"license": "MIT",
	"type": "project",
	"authors": [
		{
			"name": "Simon Gilli",
			"email": "simon.gilli@gilbertsoft.org",
			"homepage": "https://gilbertsoft.org",
			"role": "developer"
		}
	],
	"homepage": "https://github.com/gilbertsoft/opsone-managed-server",
	"require": {
		"php": "^7.0 || ^8.0",
		"gilbertsoft/opsone-managed-server": "^1.0.0"
	},
	"require-dev": {
		"symfony/polyfill-apcu": "*",
		"symfony/polyfill-ctype": "*",
		"symfony/polyfill-iconv": "*",
		"symfony/polyfill-intl-grapheme": "*",
		"symfony/polyfill-intl-icu": "*",
		"symfony/polyfill-intl-idn": "*",
		"symfony/polyfill-intl-messageformatter": "*",
		"symfony/polyfill-intl-normalizer": "*",
		"symfony/polyfill-mbstring": "*",
		"symfony/polyfill-php70": "*",
		"symfony/polyfill-php71": "*",
		"symfony/polyfill-php72": "*",
		"symfony/polyfill-php73": "*",
		"symfony/polyfill-php74": "*",
		"symfony/polyfill-php80": "*",
		"symfony/polyfill-php81": "*",
		"symfony/polyfill-uuid": "*"
	},
	"repositories": [
		{
			"type": "path",
			"url": "../../..",
			"options": {
				"versions": {
					"gilbertsoft/opsone-managed-server": "1.0.0"
				}
			}
		}
	],
	"config": {
		"allow-plugins": {
			"ergebnis/composer-normalize": true
		},
		"discard-changes": true,
		"sort-packages": true
	}
}
